  <preset name="feg" fwd="feg.16_10"/>
  <preset name="feg.16_10">
    <black/>
    <ccli/>
    <linesPerPage>13</linesPerPage>
    <set-text draw:style-name="gr1" svg:x="0.6cm" svg:y="-0.15cm" svg:width="30.8cm" svg:height="19.05cm"/> <!-- gr1 / gr2 / gr3 to choose center/top/bottom -->
    <set-rights svg:x="0.6cm" svg:y="19.66cm" svg:width="18.5cm" svg:height="0cm"/>
    <set-from svg:x="21.7cm" svg:y="20.1cm" svg:width="8cm" svg:height="0cm"/>
