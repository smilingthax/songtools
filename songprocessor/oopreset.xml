    <set-pgof svg:x="30.4cm" svg:y="20.1cm" svg:width="1.6cm" svg:height="0cm"/>
    <set-fullimg x="0" y="0" width="32" height="20"/> <!-- no units, no svg: namespace! -->
    <set-partimg x="0.85" y="0.1" width="31.3" height="18.7" xpos="0" aspectHack="0.25"/> <!-- y: trial and error; height: only top part used;  note: usually @border=1 => 0.5 padding everywhere [except: Celtic Alleluja] ; note: our text-boxes have 0.25 padding left; xpos=0: border is also redistributed! -->
    <premaster style:page-layout-name="PM4"/> <!-- 16:10 with 32cm x 20cm -->
    <poststatic>
      <draw:frame draw:style-name="gr3" draw:text-style-name="P1" draw:layer="layout" svg:width="10.5cm" svg:height="0cm" svg:x="0.6cm" svg:y="20.1cm">
        <draw:text-box>
          <text:p text:style-name="P1">Nur für den gottesdienstlichen Gebrauch in der FeG</text:p>
        </draw:text-box>
      </draw:frame>
    </poststatic>
  </preset>
