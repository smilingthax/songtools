  </preset>
  <preset name="nok"><!-- 16:9 (via PM3) -->
    <black/>
    <ccli/>
    <linesPerPage>8.5</linesPerPage>
    <copy fromhref="nok-bg.jpg" tohref="Pictures/nok-bg.jpg" mime="image/jpeg"/>
    <set-text draw:style-name="gr2" svg:x="5.0cm" svg:y="0.5cm" svg:width="28.0cm" svg:height="15.13cm"/>
    <set-rights svg:x="5.0cm" svg:y="20.66cm" svg:width="15.0cm" svg:height="0cm"/>
    <!-- no <set-from/>, <set-lb/> -->
    <set-pgof svg:x="30.77cm" svg:y="21.1cm" svg:width="1.5cm" svg:height="0cm"/>
    <set-fullimg x="0" y="0" width="37.33" height="21"/>
    <set-partimg x="4.5" y="0.6" width="28" height="19.23" ypos="0"/> <!-- xpos/ypos: 0 left, 0.5 center(default), 1 right -->
    <premaster style:page-layout-name="PM3">
      <draw:frame draw:style-name="gr2" draw:text-style-name="P2" draw:layer="layout" svg:width="21.0cm" svg:height="4.5cm" draw:transform="rotate (1.5707963267946) translate (0cm 21.0cm)">
        <draw:image xlink:href="Pictures/nok-bg.jpg" xlink:type="simple" xlink:show="embed" xlink:actuate="onLoad"/>
      </draw:frame>
    </premaster>
    <poststatic>
      <draw:frame draw:style-name="gr3" draw:text-style-name="P1" draw:layer="layout" svg:width="8.5cm" svg:height="0cm" svg:x="5.0cm" svg:y="21.1cm">
        <draw:text-box>
          <text:p text:style-name="P1">Nur für den gottesdienstlichen Gebrauch in der FeG</text:p>
        </draw:text-box>
      </draw:frame>
    </poststatic>
  </preset>
