    <linesPerPage>10</linesPerPage>
    <copy fromhref="smd_claim-onblack.png" tohref="Pictures/smd_claim-onblack.png" mime="image/png"/>
    <set-text draw:style-name="gr2" svg:x="0.5cm" svg:y="0.0cm" svg:width="27.0cm" svg:height="19.13cm"/>
